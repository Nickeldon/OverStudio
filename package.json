{
  "name": "overstudio",
  "version": "1.5.0.0",
  "description": "Listen to music in a new way",
  "main": "src/electron.js",
  "scripts": {
    "package-win": "electron-packager . --overwrite --platform=win32 --icon=Addons/logo/logowin.ico --arch=ia32 --prune=true --out=OverStudio",
    "package-win_x64": "electron-packager . --overwrite --platform=win32 --icon=Addons/logo/logowin.ico --arch=x64 --prune=true --out=OverStudio",
    "package-linux": "electron-packager . --overwrite --platform=linux --icon=Addons/logo/logowin.png --arch=x64 --prune=true --out=OverStudio",
    "start": "electron ."
  },
  "author": "Nickeldon",
  "license": "ISC",
  "dependencies": {
    "auto-git-update": "^2.1.2",
    "cors": "^2.8.5",
    "electron-reload": "^2.0.0-alpha.1",
    "express": "^4.19.2",
    "jsmediatags": "^3.9.7",
    "update-electron-app": "^3.0.0",
    "url": "^0.11.3",
    "ws": "^8.16.0",
    "youtube-sr": "^4.3.11"
  },
  "devDependencies": {
    "electron": "^30.0.0"
  },
  "packagerConfig": {
      "executableName": "overstudio"
  }
}
